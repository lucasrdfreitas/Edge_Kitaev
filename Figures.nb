   RowBox[{"D_", ",", "n_"}], "]"}], " ", ":=", " ", 
  RowBox[{"D", "\[LeftDoubleBracket]", " ", 
   RowBox[{"{", 
    RowBox[{"1", ",", "n", ",", 
     RowBox[{"n", "+", "5"}], ",", 
     RowBox[{
      RowBox[{"2", "n"}], "+", "4"}]}], "}"}], " ", "\[RightDoubleBracket]"}],
   " "}]], "Input",
 CellChangeTimes->{{3.799774544697686*^9, 3.7997745458208427`*^9}, {
